<script>
    let browser = prompt("what was your browser?","");
    switch (browser) {

        case 'Chrome':
        case 'Firefox':
        case 'Safari':
        case 'Opera':
            alert('Okay we support these browsers too');
            break;

        case 'Edge':
            alert("You've got the Edge!");
            break;

        default:
            alert('We hope that this page looks ok!');
    }
</script>

